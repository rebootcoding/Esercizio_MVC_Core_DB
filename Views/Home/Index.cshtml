@model IEnumerable<Staff>

<p>
	@Html.ActionLink("Create new", "Create")
</p>
<table class="table">
	<tr>
		<th>
			@Html.DisplayNameFor(item => item.FirstName)
		</th>
		<th>
			@Html.DisplayNameFor(item => item.LastName)
		</th>
		<th>
			@Html.DisplayNameFor(item => item.StaffId)
		</th>
		<th></th>
		<th></th>
	</tr>

	@foreach (var item in Model)
	{
		<tr>
			<td>
				@Html.DisplayFor(modelItem => item.FirstName)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.LastName)
			</td>
			<td>
				@Html.DisplayFor(modelItem => item.StaffId)
			</td>
			<td>
				@Html.ActionLink("Edit", "Edit", new {id = item.StaffId})
			</td>
			<td>
				@Html.ActionLink("Delete", "Delete", new {id = item.StaffId})
			</td>
		</tr>
	}
</table>